modules = ["python-3.11"]
[agent]
expertMode = true
integrations = ["python_database:1.0.0", "web_scraper:1.0.0", "flask_authentication:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Flask EAD Platform"

[[workflows.workflow]]
name = "Flask EAD Platform"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python app.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80
